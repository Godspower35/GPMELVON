<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_users_form_md_field" model="ir.ui.view">
            <field name="name">res.users.form.md.field</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="base.view_users_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="after">
                    <group string="MD Approval" name="md_approval">
                        <field name="is_md"/>
                    </group>
                </xpath>
            </field>
        </record>
    </data>
</odoo>